{
    "name": "@launchmenu/test-applet",
    "version": "0.0.0",
    "description": "A test applet for LaunchMenu",
    "types": "build/index.d.ts",
    "files": [
        "build/**/*.{js,d.ts}"
    ],
    "main": "build/applet.js",
    "scripts": {
        "postinstall": "npm build",
        "build": "lm-build-tools --build --production",
        "dev": "lm-build-tools --watch",
        "start": "lm-build-tools --launch --production",
        "start:dev": "lm-build-tools --launch",
        "clean": "lm-build-tools --cleanup",
        "test": "jest -i",
        "test-watch": "jest --watch -i",
        "test-watch-debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch --config=\"jest.config.js\""
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/LaunchMenu/LaunchMenu.git"
    },
    "keywords": [
        "LaunchMenu",
        "Test",
        "Applet"
    ],
    "author": "Tar van Krieken",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/LaunchMenu/LaunchMenu/issues"
    },
    "homepage": "https://github.com/LaunchMenu/LaunchMenu#readme",
    "dependencies": {},
    "peerDependencies": {
        "launchmenu/launchmenu": "^0.0.0",
        "react": "^16.13.0"
    },
    "devDependencies": {
        "launchmenu/build-tools": "^0.0.0",
        "@testing-library/jest-dom": "^5.5.0",
        "@testing-library/react": "^10.0.3",
        "@types/jest": "^26.0.0",
        "@types/react": "^16.9.35",
        "babel-loader": "^8.1.0",
        "concurrently": "^5.2.0",
        "copy-webpack-plugin": "^6.0.2",
        "file-loader": "^6.0.0",
        "jest": "^26.0.1",
        "nodemon": "^2.0.4",
        "nodemon-webpack-plugin": "^4.3.2",
        "raw-loader": "^4.0.1",
        "ts-jest": "^26.1.0",
        "ts-loader": "^7.0.5",
        "typescript": "4.0.2",
        "webpack": "^4.43.0",
        "webpack-cli": "^3.3.12",
        "webpack-dev-server": "^3.11.0",
        "write-file-webpack-plugin": "^4.5.1"
    }
}
